[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[core]
	compression = 0
	excludesfile = ~/.config/git/global-gitignore
	ignorecase = false
[pull]
	rebase = true
[init]
	defaultBranch = main
	templatedir = ~/.config/git/template
[include]
    path = local-config
